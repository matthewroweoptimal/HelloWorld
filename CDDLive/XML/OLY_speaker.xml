				</Effect>
				<Param	name="fader"	format="float"	default="0"  min="-120"  max="10" />
				<Param	name="mute" 	format="bool"	default="false" />
				<Param	name="invert"	format="bool"	default="false" />
				<Effect name="softlimit">
					<Param	name="enable"	format="bool"	default="false" />
					<Param	name="gain" 	format="float"	default="0" 	min="0" 	max="20" />
					<Param	name="thresh"	format="float"	default="0" 	min="-80"	max="0" />
					<Param	name="attack"	format="float"	default="0.1"	min="0.1"	max="20000" />
					<Param	name="release"	format="float"	default="250"	min="50"	max="20000" />
					<Param	name="ratio"	format="float"	default="2" 	min="1" 	max="101" />
					<Param	name="knee" 	format="knee"	default="hard" />

					<Meter	name="in"		  type="normal" />
					<Meter	name="gainreduce" type="compgainred" />
					<Meter	name="out"		  type="normal" />
				</Effect>
				<Effect name="klimiter">
					<Param	name="peakenable"	  format="bool"   default="false" />
					<Param	name="peakattack"	  format="float"  default="16.0"		  min="0.1"   max="20000.0" />
					<Param	name="gainrelease"	  format="float"  default="100.0"		  min="1.0"   max="20000.0" />
					<Param	name="lookahead"	  format="float"  default="0.0" 		  min="0.0"   max="3.0" />
					<Param	name="maxpeak"		  format="float"  default="0.655390286"   min="0.0"   max="1.0" />
					<Param	name="clipenable"	  format="bool"   default="false" />
					<Param	name="clipattack"	  format="float"  default="1.0" 		  min="0.1"   max="3.0" />
					<Param	name="cliprelease"	  format="float"  default="30.0"		  min="1.0"   max="300.0" />
					<Param	name="clipthreshold"  format="float"  default="0.655390286"   min="0.0"   max="1.0" />

					<Meter	name="in"		  type="normal" />
					<Meter	name="gainreduce" type="compgainred" />
					<Meter	name="out"		  type="normal" />
				</Effect>
				<Effect name="peq_post"  instance="2">
					<Param name="enable"	format="bool"	default="true" />
					<Qual	name="band" instances="4">
						<Param	name="type" format="eq" 	default="parametric" />
						<Param	name="gain" format="float"	default="0" 	min="-15"	max="15" />
						<Param	name="freq" format="float"	default="1000" min="20"    max="20000" />
						<Param	name="q"	format="float"	default="2" 	min="0.1"	max="64" />
					</Qual>
				</Effect>
				<Effect name="dyneq"  instances="2">
					<Param	name="detecten" format="bool" default="false" />
					<Param	name="detecttype" format="eq"	  default="parametric" />
					<Param	name="detectgain" format="float"  default="0"		min="-15"	max="15" />
					<Param	name="detectfreq" format="float"  default="250" 	min="20"	max="20000" />
					<Param	name="detectq"	  format="float"  default="2"		min="0.1"	max="64" />

					<Param	name="agcenable"  format="bool"   default="false" />
					<Param	name="agcattack"  format="float"  default="0.5" 	min="0.1"	max="10000.0" />
					<Param	name="agcdecay"   format="float"  default="20"		min="1" 	max="10000" />
					<Param	name="agcactive"  format="bool"   default="false" />
					<Param	name="agcthresh"  format="float"  default="-40" 	min="-60"	max="60" />
					<Param	name="agcratio"   format="float"  default="1"		min="1" 	max="201" />

					<Param	name="resumen" format="bool" default="false" />
					<Param	name="resumtype" format="eq"	 default="parametric" />
					<Param	name="resumgain" format="float"  default="0"		min="-15"	max="15" />
					<Param	name="resumfreq" format="float"  default="250"		min="20"	max="20000" />
					<Param	name="resumq"	 format="float"  default="2.5"		min="0.1"	max="64" />
				</Effect>			 
				<Effect name="delay">
					<Param	name="enable"	format="bool"	default="true" />
					<Param	name="time" 	format="float"	default="0" 	  min="0"	max="10.0" />
				</Effect>
				<Param	name="fader_post"	 format="float"  default="0"  min="-120"  max="10" />
				<Param	name="mute_post"	 format="bool"	 default="false" />
				<Meter	name="out"	 type="normal" />
			</Channel>
			<Channel	name="amp"	instances="4">
				<Param	name="routing"	format="ampsrc" default="LF"/>
			</Channel>
			<!-- hack to get dynamic eq meters at the end -->
			<Channel name="HF_DynEQ"  instances="2">
				<Meter	name="gain"	 type="dyneqgain" />
			</Channel>
			<Channel name="MF_DynEQ"  instances="2">
				<Meter	name="gain"	 type="dyneqgain" />
			</Channel>
			<Channel name="LF_DynEQ"  instances="2">
				<Meter	name="gain"	 type="dyneqgain" />
			</Channel>
		</Segment>
	  
		<Segment	name="FIR">
			<Channel	name="HF">
				<Effect name="fir">
					<Param	name="enable"	format="bool"	default="true" />
					<Param	name="mute" 	format="bool"	default="false" />
					<Qual	name="coef" instances="600">
						<Param	name="value" format="float"  default="0"	 min="-2.0"   max="2.0" />
					</Qual>
				</Effect>
			</Channel>
			<Channel	name="MF">
				<Effect name="fir">
					<Param	name="enable"	format="bool"	default="true" />
					<Param	name="mute" 	format="bool"	default="false" />
					<Qual	name="coef" instances="600">
						<Param	name="value" format="float"  default="0"	 min="-2.0"   max="2.0" />
					</Qual>
				</Effect>
			</Channel>
			<Channel	name="LF">
				<Effect name="fir">
					<Param	name="enable"	format="bool"	default="true" />
					<Param	name="mute" 	format="bool"	default="false" />
					<Qual	name="coef" instances="600">
						<Param	name="value" format="float"  default="0"	 min="-2.0"   max="2.0" />
					</Qual>
				</Effect>
			</Channel>
		</Segment>
		<Segment	name="xover">
			<Channel	name="speaker">
				<Effect name="xover">
					<Param	name="type"			format="eq" 		default="hi_pass" />
					<Param	name="enable"		format="bool"		default="true" />
					<Param  name="freq"     format="float"  default="20"    min="20"    max="20000" />
					<Param  name="slope"    format="slope"  default="slope_18" />
					<Param  name="response" format="response"   default="butterworth" />
					<Param	name="hpfoutenable"		format="bool"		default="false" />
					<Param	name="override_fir_disable" format="bool" default="false" />
				</Effect>
			</Channel>
		</Segment>
		<Segment	name="airloss">
			<Effect name="eq">
				<Param	name="enable"		format="bool"		default="false" />
				<Param  name="freq"     format="float"  default="100000"    min="20"    max="100000" />
			</Effect>
		</Segment>
		<Segment	name="noisegate">
			<Channel	name="input">
				<Effect name="gate">
					<Param  name="enable"   format="bool"   default="false" />
					<Param  name="thresh"   format="float"  default="-80"   min="-120"   max="0" />
					<Param  name="attack"   format="float"  default="0.1"   min="0.1"   max="300"  />
					<Param  name="release"  format="float"  default="250"   min="50"    max="3000" />
					<Param  name="range"    format="float"  default="60"    min="0"     max="60" />
					<Param  name="hold"     format="float"  default="0"     min="0"     max="5000" />

					<Meter  name="in"         type="normal" />
					<Meter  name="gainreduce" type="gategainred" />
					<Meter  name="out"        type="normal" />
				</Effect>
			</Channel>
		</Segment>
	</Device>	

